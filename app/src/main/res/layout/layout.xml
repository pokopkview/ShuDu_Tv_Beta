<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/container"
    android:layout_width="wrap_content"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <FrameLayout
        android:id="@+id/cell_container"
        android:layout_width="@dimen/px1053"
        android:layout_height="@dimen/px1053"
        android:layout_centerInParent="true">

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_1"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_2"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_3"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_4"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:focusable="true"
                    android:id="@+id/sd_5"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_6"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_weight="1">

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_7"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_8"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />

                <zhang.great.tv.shudu_tv_beta.Base_Cell.SDSUView
                    android:id="@+id/sd_9"
                    android:layout_width="@dimen/px351"
                    android:layout_height="@dimen/px351" />
            </LinearLayout>
        </LinearLayout>
    </FrameLayout>



</RelativeLayout>
